<?xml version="1.0" ?>

<launch>    

    <param name="simulation_check"       value="false" type="bool"/>
    <param name="simulation_robot_speed" value="0.2"/>  <!--m/s-->
    <param name="robot_trj_time"         value="1.0"/>  <!--s-->
    <param name="detect_distance"        value="4.0"/>  <!--m-->
    
    <param name="lidar_detect_angle"     value="15.0"/>  <!--degree-->
    <param name="lidar_detect_distance"  value="0.3"/>  <!--m-->
    <param name="lidar_sampling_count"   value="15" type="int"/>  <!--m-->
 
     <!-- erica Manager -->
    <node name="erica_decision_node" pkg="erica_decision" type="erica_decision_node" output="screen"/>


</launch>
